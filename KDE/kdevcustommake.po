#, kde-format
msgid "Makefile is missing in folder \"%1\""
msgstr "“%1” klasöründe Makefile yok"

#: makefileresolver/makefileresolver.cpp:276
#, kde-format
msgid "Problem while trying to resolve include paths for %1"
msgstr "%1 için “include” dosya yolları çözümlenirken sorun"

#: makefileresolver/makefileresolver.cpp:310
#, kde-format
msgid "Cached: %1"
msgstr "Önbelleklenen: %1"

#: makefileresolver/makefileresolver.cpp:339
#, kde-format
msgid "Filename %1 seems to be malformed"
msgstr "%1 dosya adı hatalı oluşturulmuş gibi görünüyor."

#: makefileresolver/makefileresolver.cpp:472
#, kde-format
msgid "Failed to extract new working directory"
msgstr "Yeni çalışma dizini çıkarılamadı."

#: makefileresolver/makefileresolver.cpp:472
#: makefileresolver/makefileresolver.cpp:501
#, kde-format
msgid "Output was: %1"
msgstr "Çıktı: %1"

#: makefileresolver/makefileresolver.cpp:494
#: makefileresolver/makefileresolver.cpp:497
#, kde-format
msgid "Recursive make call failed"
