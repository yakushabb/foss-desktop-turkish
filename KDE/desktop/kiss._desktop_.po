#: files/kde-initial-system-setup.desktop.in:9
#: src/auth/org.kde.initialsystemsetup.actions:8
msgctxt "Name"
msgid "KDE Initial System Setup"
msgstr "KDE İlk Kullanım Ayarları"

#: files/kde-initial-system-setup.desktop.in:28
msgctxt "Comment"
msgid "Run the initial system setup for KDE"
msgstr "KDE için ilk sistem ayarlarını yapın"

#: src/auth/org.kde.initialsystemsetup.actions:31
msgctxt "Name"
msgid "Create New User Autostart Hook"
msgstr "Yeni Kullanıcı Kendiliğinden Başlatma Kancası Oluştur"

#: src/auth/org.kde.initialsystemsetup.actions:49
msgctxt "Description"
msgid "Create an autostart hook for the newly created user"
msgstr ""
"Yeni oluşturulan kullanıcı için bir kendiliğinden başlatma kancası oluşturun"

#: src/auth/org.kde.initialsystemsetup.actions:70
msgctxt "Name"
msgid "Disable Systemd Unit"
msgstr "Systemd Birimini Devre Dışı Bırak"

#: src/auth/org.kde.initialsystemsetup.actions:88
msgctxt "Description"
msgid "Disable the systemd unit for the initial system setup"
msgstr "İlk sistem ayarları için systemd birimini devre dışı bırakın"

#: src/auth/org.kde.initialsystemsetup.actions:109
msgctxt "Name"
msgid "Remove Autologin"
msgstr "Kendiliğinden Oturum Açmayı Kaldır"

#: src/auth/org.kde.initialsystemsetup.actions:127
msgctxt "Description"
msgid "Remove SDDM autologin configuration"
msgstr "SDDM kendiliğinden oturum açma yapılandırmasını kaldırın"

#: src/auth/org.kde.initialsystemsetup.actions:148
msgctxt "Name"
msgid "Set New User Home Directory Ownership"
msgstr "Yeni Kullanıcı Ana Klasör Sahipliğini Ayarla"

#: src/auth/org.kde.initialsystemsetup.actions:166
msgctxt "Description"
msgid "Set ownership of the new users home directory to the newly created user"
msgstr ""
"Yeni kullanıcının ana klasör sahipliğini yeni oluşturulan kullanıcıya verin"

#: src/auth/org.kde.initialsystemsetup.actions:187
msgctxt "Name"
msgid "Set New User Temporary Autologin"
msgstr "Yeni Kullanıcı Geçici Kendliğinden Oturum Açmayı Ayarla"

#: src/auth/org.kde.initialsystemsetup.actions:205
msgctxt "Description"
msgid "Set SDDM temporary autologin configuration for the newly created user"
msgstr ""
"Yeni oluşturulan kullanıcı için SDDM geçici kendiliğinden oturum açma "
"yapılandırmasını ayarlayın"
