#: daemon/chargethreshold_helper_actions.actions:62
msgctxt "Name"
msgid "Get current battery charge limit"
msgstr "Geçerli Pil Şarj Sınırını Al"

#: daemon/chargethreshold_helper_actions.actions:109
msgctxt "Description"
msgid "Authenticate to see the current battery charge limit."
msgstr "Geçerli pil şarj sınırını görmek için kimliğinizi doğrulayın."

#: daemon/chargethreshold_helper_actions.actions:154
msgctxt "Name"
msgid "Set battery charge limit"
msgstr "Pil Şarj Sınırı Ayarla"

#: daemon/chargethreshold_helper_actions.actions:201
msgctxt "Description"
msgid "Authenticate to set the battery charge limit."
msgstr "Pil şarj sınırını ayarlamak için kimliğinizi doğrulayın."

#: daemon/chargethreshold_helper_actions.actions:246
msgctxt "Name"
msgid "Check battery conservation mode"
msgstr "Pil Tasarrufu Kipini Denetle"

#: daemon/chargethreshold_helper_actions.actions:286
msgctxt "Description"
msgid "Authenticate to check the current state of battery conservation mode."
msgstr ""
"Pil tasarrufu kipinin geçerli durumunu denetlemek için kimliğinizi "
"doğrulayın."

#: daemon/chargethreshold_helper_actions.actions:329
msgctxt "Name"
msgid "Set battery conservation mode"
msgstr "Pil Tasarrufu Kipini Ayarla"

#: daemon/chargethreshold_helper_actions.actions:369
msgctxt "Description"
msgid "Authenticate to set the battery conservation mode."
msgstr "Pil tasarrufu kipini ayarlamak için kimliğinizi doğrulayın."

#: daemon/controllers/backlight_helper_actions.actions:62
msgctxt "Name"
msgid "Get maximum brightness value"
msgstr "En Yüksek Parlaklık Değerini Al"

#: daemon/controllers/backlight_helper_actions.actions:118
msgctxt "Description"
msgid "Authenticate to see the maximum brightness level."
msgstr "En yüksek parlaklık değerini görmek için kimliğinizi doğrulayın."

#: daemon/controllers/backlight_helper_actions.actions:163
msgctxt "Name"
msgid "Get brightness"
msgstr "Parlaklığı Al"

#: daemon/controllers/backlight_helper_actions.actions:219
msgctxt "Description"
msgid "Authenticate to see the brightness level."
msgstr "Parlaklık değerini görmek için kimliğinizi doğrulayın."

#: daemon/controllers/backlight_helper_actions.actions:264
msgctxt "Name"
msgid "Set brightness"
msgstr "Parlaklığı Ayarla"

#: daemon/controllers/backlight_helper_actions.actions:320
msgctxt "Description"
msgid "Authenticate to set the brightness level."
msgstr "Parlaklık değerini ayarlamak için kimliğinizi doğrulayın."

#: daemon/controllers/backlight_helper_actions.actions:364
msgctxt "Name"
msgid "Get syspath"
msgstr "syspath’i Al"

#: daemon/controllers/backlight_helper_actions.actions:418
msgctxt "Description"
msgid "Authenticate to see the syspath."
msgstr "syspath’i görmek için kimliğinizi doğrulayın."

#: daemon/discretegpu_helper_actions.actions:62
msgctxt "Name"
msgid "Check for existence of discrete GPU"
msgstr "Ayrı GPU’nun Varlığı Denetle"

#: daemon/discretegpu_helper_actions.actions:104
msgctxt "Description"
msgid "Authenticate to check for the existence of a discrete GPU."
msgstr "Ayrı bir GPU’nun varlığını denetlemek için kimliğinizi doğrulayın."

#: daemon/powerdevil.desktop.cmake:4 powerdevil.notifyrc:2
msgctxt "Name"
msgid "Power Management"
msgstr "Güç Yönetimi"

#: daemon/powerdevil.desktop.cmake:65
msgctxt "Comment"
msgid "Battery, Display and CPU power management and notification"
msgstr "Pil, Görüntü ve İşlemci güç yönetimi ve bildirimleri"

#: powerdevil.notifyrc:58
msgctxt "Comment"
msgid "Power Management System"
msgstr "Güç Yönetimi Sistemi"

#: powerdevil.notifyrc:116
msgctxt "Name"
msgid "Warning"
msgstr "Uyarı"

#: powerdevil.notifyrc:171
msgctxt "Comment"
msgid "Used for warning notifications"
msgstr "Uyarı bildirimleri için kullanılır"

#: powerdevil.notifyrc:228
msgctxt "Name"
msgid "Notification"
msgstr "Bildirim"

#: powerdevil.notifyrc:284
msgctxt "Comment"
msgid "Used for standard notifications"
msgstr "Standart bildirimler için kullanılır"

#: powerdevil.notifyrc:341
msgctxt "Name"
msgid "Critical notification"
msgstr "Kritik Bildirim"

#: powerdevil.notifyrc:396
msgctxt "Comment"
msgid "Notifies a critical event"
msgstr "Kritik bir durumu bildirir"

#: powerdevil.notifyrc:453
msgctxt "Name"
msgid "Battery Low"
msgstr "Pil Düşük"

#: powerdevil.notifyrc:508
msgctxt "Comment"
msgid "Battery has reached low level"
msgstr "Pil düşük düzeye indi"

#: powerdevil.notifyrc:552
msgctxt "Name"
msgid "Battery Critical"
msgstr "Pil Kritik"

#: powerdevil.notifyrc:607
msgctxt "Comment"
msgid ""
"Battery has reached critical level. This notification triggers a countdown "
"before doing the configured action, hence it is strongly advised to leave it "
"on."
msgstr ""
"Pil kritik düzeye indi. Bu bildirim yapılandırılmış bir işlemi "
"gerçekleştirecek bir sayacı tetikler; bu nedenle olduğu gibi bırakmanız "
"önerilir."

#: powerdevil.notifyrc:652
msgctxt "Name"
msgid "Charge Complete"
msgstr "Şarj Doldu"

#: powerdevil.notifyrc:706
msgctxt "Comment"
msgid "The battery is fully charged"
msgstr "Piliniz tümüyle şarj edildi."

#: powerdevil.notifyrc:766
msgctxt "Name"
msgid "Peripheral Battery Low"
msgstr "Çevre Birimi Pili Düşük"

#: powerdevil.notifyrc:818
msgctxt "Comment"
msgid "The battery in a connected device, such as mouse or keyboard, is low"
msgstr "Fare veya klavye gibi bağlı bir aygıttaki pil düşük"

#: powerdevil.notifyrc:876
msgctxt "Name"
msgid "Power source connected"
msgstr "Güç Kaynağı Bağlandı"

#: powerdevil.notifyrc:923
msgctxt "Comment"
msgid "The power source has been connected"
msgstr "Güç kaynağı bağlandı"

#: powerdevil.notifyrc:976
msgctxt "Name"
msgid "Power source disconnected"
msgstr "Güç Kaynağı Bağlantısı Kesildi"

#: powerdevil.notifyrc:1023
msgctxt "Comment"
msgid "The power source has been disconnected"
msgstr "Güç kaynağı bağlantısı kesildi"

